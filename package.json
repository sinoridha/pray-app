{
  "name": "zikir-indonesia",
  "version": "0.1.1",
  "description": "Zikir Indonesia Beta 1",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "pack": "build --dir",
    "dist": "build"
  },
  "repository": {
    "type": "git",
    "url": "pray-app"
  },
  "keywords": [
    "pray",
    "mac",
    "osx",
    "electron"
  ],
  "author": "Ridha Ramadhansyah",
  "license": "MIT",
  "build": {
    "appId": "sino.ridha.prayapp5",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    }
  },
  "devDependencies": {
    "electron": "1.7.11",
    "electron-builder": "^19.56.2"
  },
  "dependencies": {
    "eslinter": "^3.2.1",
    "moment": "^2.29.4",
    "node-schedule": "^1.3.0",
    "ontime": "0.0.4"
  }
}
